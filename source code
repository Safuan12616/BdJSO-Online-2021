function getCookie(cname) {
	var name = cname + "=";
	var decodedCookie = decodeURIComponent(document.cookie);
	var ca = decodedCookie.split(';');
	for(var i = 0; i <ca.length; i++) {
		var c = ca[i];
		while (c.charAt(0) == ' ') {
			c = c.substring(1);
		}
		if (c.indexOf(name) == 0) {
			return c.substring(name.length, c.length);
		}
	}
	return "";
}

</script>

<script>
$(document).ready(function(){
    $("form").submit(function(e){
        e.preventDefault();

        formId = $('form').attr('id');
    });
});

function buttonSubmit(formId) {
    var formId_value = formId;
    var actionurl = 'submission.php';
    var form_id = "#form-"+formId;
    var accesstoken = getCookie('accesstoken');
    var data = $(form_id).serialize() + "&accesstoken="+accesstoken+"&formId="+formId_value;
    // var answer_value = $(form_id+' input[name="answer"]').val();
    var answer_value = $('.'+formId_value+'answer_field').val();
    var answer_value_1 = $('.'+formId_value+'answer_field_1').val();
    var answer_value_2 = $('.'+formId_value+'answer_field_2').val();
    var answer_value_3 = $('.'+formId_value+'answer_field_3').val();
    var answer_value_4 = $('.'+formId_value+'answer_field_4').val();

    // console.log('Submission');
    // console.log(formId);
    // console.log(data);
    // console.log(answer_value);

    if (answer_value || answer_value_1 || answer_value_2 || answer_value_3 || answer_value_4) {
        $.ajax({
            type: "POST",
            url: actionurl,
            data: data,
            success: function (data) {

                if (data.status == 'success') {
                    var title = data.status;

                    $("#info-"+data.formId).css('display', 'block');
                    $("#info-"+data.formId).text();
                    $("#info-"+data.formId).text("Ans: "+data.message);

                    iziToast.success({
                        title: title.toUpperCase(),
                        message: data.message
                    });
                } else if (data.status == 'error') {
                    var title = data.status;

                    iziToast.error({
                        title: title.toUpperCase(),
                        message: data.message
                    });
                } else {
                    // console.log(data);
                }
                // console.log(data);

            },
            error: function (jqXhr, textStatus, errorMessage) {
                console.log("Error: ", errorMessage);
                iziToast.error({
                    title: 'Submission Failed! Please refresh the page!',
                    message: errorMessage
                });
            }
        });
    } else {
        iziToast.error({
            title: 'Failed!',
            message: 'No Value Submitted!'
        });
    }

}

document.addEventListener('contextmenu', event => event.preventDefault());

// Set the date we're counting down to
var countDownDate = new Date("2021-08-04 12:00:00").getTime();

// Update the count down every 1 second
var x = setInterval(function() {

    // Get today's date and time
    var now = new Date().getTime();

    // Find the distance between now and the count down date
    var distance = countDownDate - now;

    // Time calculations for days, hours, minutes and seconds
    var days = Math.floor(distance / (1000 * 60 * 60 * 24));
    var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
    var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
    var seconds = Math.floor((distance % (1000 * 60)) / 1000);

    // Output the result in an element with id="demo"
    document.getElementById("countdown_timer").innerHTML = hours + "h "
    + minutes + "m " + seconds + "s ";

    // If the count down is over, write some text
    if (distance < 0) {
        clearInterval(x);
        document.getElementById("countdown_timer").innerHTML = "EXPIRED";
    }
}, 1000);
</script>

